# PromptPolish Environment Variables

# API Keys
# Required: OpenRouter API key for accessing AI models
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Optional: Anthropic API key (if you want to use Anthropic directly instead of through OpenRouter)
# ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Optional: OpenAI API key (if you want to use OpenAI directly instead of through OpenRouter)
# OPENAI_API_KEY=your_openai_api_key_here

# Server Configuration
PORT=5000

# Development Settings
NODE_ENV=development